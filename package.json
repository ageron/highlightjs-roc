{
  "name": "highlightjs-roc",
  "version": "0.1.0",
  "description": "highlight.js syntax definition for Roc",
  "main": "src/roc.js",
  "module": "dist/roc.es.min.js",
  "files": [
    "src",
    "styles",
    "dist"
  ],
  "scripts": {
    "build:cjs": "webpack --config webpack.cjs.config.js",
    "build:esm": "webpack --config webpack.esm.config.js",
    "build": "npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isaacvando/highlightjs-roc.git"
  },
  "keywords": [
    "roc",
    "highlight.js",
    "highlightjs",
    "syntax",
    "highlighting",
    "language"
  ],
  "author": "Isaac Van Doren",
  "license": "UPL",
  "bugs": {
    "url": "https://github.com/isaacvando/highlightjs-roc/issues"
  },
  "homepage": "https://github.com/isaacvando/highlightjs-roc",
  "peerDependencies": {
    "highlight.js": "^11.0.0"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
